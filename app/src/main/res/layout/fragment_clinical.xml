<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor">

    <androidx.cardview.widget.CardView
        android:id="@+id/boxTimeDate"
        android:layout_width="250dp"
        android:layout_height="70dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:cardBackgroundColor="@color/boxViewColor">

        <TextClock
            android:id="@+id/timeView"
            android:text="@string/_08_17"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="5dp"
            android:textColor="#ffffff"
            android:textSize="30sp"
            android:textAlignment="center"
            android:gravity="center"
            tools:targetApi="jelly_bean_mr1" />

        <TextClock
            android:id="@+id/dateView"
            android:text="@string/_11_mei_2020"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="40dp"
            android:textColor="#ffffff"
            android:textAlignment="center"
            android:textSize="15sp"
            android:gravity="center"
            tools:targetApi="jelly_bean_mr1" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/boxInputValue"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boxTimeDate"
        android:layout_marginTop="10dp"
        app:cardBackgroundColor="@color/boxViewColor">

        <TextView
            android:text="@string/setup_for_pmax"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="5dp"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="21sp"
            android:textStyle="bold"
            android:gravity="center"/>

        <TextView
            android:text="@string/fio2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="45dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inputFI02"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="70dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textAlignment="center"
            android:textStyle="bold"
            android:lines="1"
            android:background="@drawable/edit_text_rounded"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:text="@string/trigger_pressure_cmhg"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="120dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inputTidalVolume"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="145dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textStyle="bold"
            android:textAlignment="center"
            android:lines="1"
            android:background="@drawable/edit_text_rounded"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:text="@string/resp_rate_i_min"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="195dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inputRespRate"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="220dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textStyle="bold"
            android:textAlignment="center"
            android:lines="1"
            android:background="@drawable/edit_text_rounded"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:text="@string/peep_cmh2o"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="270dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inpuPEEP"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="295dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textStyle="bold"
            android:textAlignment="center"
            android:lines="1"
            android:background="@drawable/edit_text_rounded"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:text="@string/i_e_ratio"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="345dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inputIE"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="370dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textStyle="bold"
            android:textAlignment="center"
            android:background="@drawable/edit_text_rounded"
            android:lines="1"
            android:layout_gravity="center_horizontal"/>

        <TextView
            android:text="@string/max_plateau_pressure_cmh2o"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="420dp"
            android:gravity="center_horizontal"
            app:fontFamily="sans-serif"
            android:textColor="#ffffff"
            android:layout_gravity="center_horizontal"
            android:textSize="17sp"/>

        <EditText
            android:id="@+id/inputMaxPlateau"
            android:layout_width="160dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="465dp"
            android:inputType="numberDecimal"
            android:textColor="#00C6C1"
            android:textAlignment="center"
            android:textStyle="bold"
            android:lines="1"
            android:background="@drawable/edit_text_rounded"
            android:layout_gravity="center_horizontal"/>

        <Button
            android:id="@+id/pmaxButton"
            android:layout_width="120dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="parent"
            android:layout_marginTop="530dp"
            android:layout_marginBottom="20dp"
            android:background="@drawable/rounded_button"
            android:text="@string/change_p_max"
            android:textSize="12sp"
            android:textColor="@color/backgroundColor"
            android:layout_gravity="center_horizontal"/>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/boxDonutChart"
        android:layout_width="1010dp"
        android:layout_height="wrap_content"
        app:cardBackgroundColor="@color/boxViewColor"
        app:layout_constraintBottom_toTopOf="@id/line_chart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/boxTimeDate">

        <TextView
            android:id="@+id/valuePeakPressure"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="68dp"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:text="@string/no_value"
            android:textColor="#ffffff"
            android:textSize="28sp"
            android:textStyle="bold"
            android:background="@drawable/rounded_box_view1"
            app:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="48dp"
            android:layout_marginTop="85dp"
            android:layout_marginBottom="20dp"
            android:gravity="center"
            android:text="@string/peak_pressure_cmh2o"
            android:textColor="#ffffff"
            android:textSize="18sp"
            app:fontFamily="sans-serif"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/valuePEEP"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="256dp"
            android:layout_marginTop="25dp"
            android:background="@drawable/rounded_box_view2"
            android:gravity="center"
            android:text="@string/no_value"
            android:textStyle="bold"
            android:textColor="#ffffff"
            android:textSize="28sp"
            app:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="235dp"
            android:layout_marginTop="85dp"
            android:gravity="center"
            android:text="@string/peep_cmh2o"
            android:textColor="#ffffff"
            android:textSize="18sp"
            app:fontFamily="sans-serif"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/valueTidalVolumeIns"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="444dp"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:text="@string/no_value"
            android:textStyle="bold"
            android:background="@drawable/rounded_box_view3"
            android:textColor="#ffffff"
            android:textSize="28sp"
            app:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="423dp"
            android:layout_marginTop="85dp"
            android:gravity="center"
            android:text="@string/tidal_volume_ins_ml"
            android:textColor="#ffffff"
            android:textSize="18sp"
            app:fontFamily="sans-serif"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/valueTidalVolumeExp"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="632dp"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:text="@string/no_value"
            android:textStyle="bold"
            android:background="@drawable/rounded_box_view4"
            android:textColor="#ffffff"
            android:textSize="28sp"
            app:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="612dp"
            android:layout_marginTop="85dp"
            android:gravity="center"
            android:text="@string/tidal_volume_exp_ml"
            android:textColor="#ffffff"
            android:textSize="18sp"
            app:fontFamily="sans-serif"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/valueMinuteVolumeExp"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="820dp"
            android:layout_marginTop="25dp"
            android:gravity="center"
            android:text="@string/no_value"
            android:textStyle="bold"
            android:background="@drawable/rounded_box_view5"
            android:textColor="#ffffff"
            android:textSize="28sp"
            app:fontFamily="sans-serif"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="800dp"
            android:layout_marginTop="85dp"
            android:gravity="center"
            android:text="@string/minute_volume_exp_ml"
            android:textColor="#ffffff"
            android:textSize="18sp"
            app:fontFamily="sans-serif"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.cardview.widget.CardView>


    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/line_chart"
        android:layout_width="850dp"
        android:layout_height="245dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/boxDonutChart"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="260dp"/>

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/line_chart1"
        android:layout_width="850dp"
        android:layout_height="245dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_chart"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="260dp"/>

    <androidx.cardview.widget.CardView
        android:id="@+id/boxRightButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/boxDonutChart"
        android:layout_marginLeft="1118dp"
        android:layout_marginTop="10dp"
        app:cardBackgroundColor="@color/backgroundColor">

        <TextView
            android:id="@+id/output"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnld="true"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:textColor="#ffffff"
            android:textSize="12sp" />

        <Button
            android:id="@+id/changeParameterButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="70dp"
            android:text="Change Parameter"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rounded_button_menu"
            android:textSize="10sp"
            android:textColor="@color/backgroundColor"/>

        <Button
            android:id="@+id/combinedButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="130dp"
            android:text="Combined"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rounded_button_menu"
            android:textSize="10sp"
            android:textColor="@color/backgroundColor"/>

        <Button
            android:id="@+id/timedButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="190dp"
            android:text="Timed"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rounded_button_menu"
            android:textSize="10sp"
            android:textColor="@color/backgroundColor"/>

        <Button
            android:id="@+id/spontanegusButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="250dp"
            android:text="Spontanegus"
            android:layout_gravity="center_horizontal"
            android:background="@drawable/rounded_button_menu"
            android:textSize="10sp"
            android:textColor="@color/backgroundColor"/>

        <Button
            android:id="@+id/startButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="340dp"
            android:text="START"
            android:layout_gravity="center_horizontal"
            android:drawableLeft="@drawable/ic_play_arrow_black_24dp"
            android:textSize="20sp"
            android:textColor="@color/backgroundColor"
            android:background="@drawable/rounded_button_start"/>

        <Button
            android:id="@+id/stopButton"
            android:layout_width="130dp"
            android:layout_height="40dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="400dp"
            android:text="STOP"
            android:layout_gravity="center_horizontal"
            android:drawableLeft="@drawable/ic_stop_black_24dp"
            android:textSize="20sp"
            android:textColor="@color/backgroundColor"
            android:background="@drawable/rounded_button_stop"/>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>